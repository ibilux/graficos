{
  "name": "chartist",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "svelte": "src/index.js",
  "types": "@types/index.d.ts",
  "files": [
    "dist",
    "src",
    "@types"
  ],
  "scripts": {
    "tsc": "tsc",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
